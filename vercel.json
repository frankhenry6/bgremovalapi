{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "functions": {
      "server.js": {
        "includeFiles": "node_modules/@imgly/background-removal-node/dist/**"
      }
    }
  }
  